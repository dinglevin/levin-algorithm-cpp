{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Google Test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/tests/leetcode/runTests",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for lldb",
          "text": "settings set target.debug-file-search-paths /usr/local/opt/llvm/lib/debug",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build"
    }
  ]
}
